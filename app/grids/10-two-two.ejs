<div class="header-container">
    <div class="container">
        <div class="row">
            <div class="column col-md-<%- size[0] || 12 %> widget-list">
                <%- column[0] %>
            </div>
        </div>
    </div>
</div>
<div class="container">
  <div class="row">
    <div class="column col-md-<%- size[1] || 6 %> widget-list"><%- column[1] %></div>
    <div class="column col-md-<%- size[2] || 6 %> widget-list"><%- column[2] %></div>
  </div>
  <div class="row">
    <div class="column col-md-<%- size[3] || 6 %> widget-list"><%- column[3] %></div>
    <div class="column col-md-<%- size[4] || 6 %> widget-list"><%- column.slice(4).join(' ') %></div>
  </div>
</div>

<div class="halfHeight cols2">
  <div class="col c60">1</div>
  <div class="col c40">2</div>
</div>
<div class="halfHeight cols2">
  <div class="col c40">3</div>
  <div class="col c60">4</div>
</div>
